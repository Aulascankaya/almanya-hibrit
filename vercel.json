{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "almanya-hibrit.vercel.app" }
      ],
      "destination": "https://www.almanyayasam.de/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
